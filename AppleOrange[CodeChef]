https://www.codechef.com/problems/APPLEORANGE  // for question refers these 


#include <iostream>
using namespace std;

int main() {
	int t ;
	cin >> t ;
	while (t--){
	    int n , m ;
	    cin >> n >> m ;
	    int remainder ;
	    while(m!=0){
	        remainder = n % m ;
	        n = m ;
	        m = remainder ;
	    }
	    cout << n << endl;
	}
	return 0;
}
